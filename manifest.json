{
    "manifest_version": 3,
    "name": "Meow",
    "description": "Turns webpage content into catspeak.",
    "version": "1.0",
    "action": {
        "default_popup": "meow.html"
    },
    "content_scripts": [
        {
            "js": ["dom-scraping/scraper.js"],
            "matches": ["<all_urls>"]
        }
    ],
    "permissions": [
        "storage"
    ]
}